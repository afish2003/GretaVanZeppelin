xml: metadata, music.
metadata: title, newline, album, newline, artist, newline, key, newline, newline+.
title: ~[#d;#a]+.
album: ~[#d;#a]+.
artist: ~[#d;#a]+.
key: ~[#d;#a]+.
music: section++(newline+).
section: type, newline, line+, -"{", -"end_of_", -~["}";#d;#a]+, -"}".
@type: -"{", -"start_of_", ~["}";#d;#a]+, -"}".
line: (text;chord)+, -newline.
-text: ~[#d;#a;"[]{}"]+.
chord: -"[",  ~["[]{}";#d;#a]+, -"]".
-newline: (#d?, #a).
-sectionBound: newline+.
-space: " ".

